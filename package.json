{
  "name": "amazon-seller-central-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Amazon Seller Central Clone - Full Stack Application",
  "scripts": {
    "start": "快速启动.bat",
    "stop": "taskkill /f /im node.exe",
    "status": "node scripts/status.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:admin": "cd backend-admin && npm run dev",
    "build": "npm run build:backend && npm run build:frontend && npm run build:admin",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build:admin": "cd backend-admin && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install && cd ../backend-admin && npm install",
    "seed": "cd backend && npm run seed",
    "domain:setup": "node setup-hosts-node.js",
    "domain:start": "node start-domain-mode.js",
    "domain:certs": "node generate-certs-node.js",
    "domain:cleanup": "node setup-hosts-node.js cleanup",
    "domain:test": "node test-domain-config.js",
    "domain:一键配置": "node 一键配置域名.js",
    "domain:切换": "node 域名切换工具.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "frontend",
    "backend",
    "backend-admin"
  ],
  "keywords": [
    "amazon",
    "seller-central",
    "clone",
    "react",
    "node",
    "typescript"
  ],
  "author": "Amazon Seller Central Clone Team",
  "license": "MIT",
  "dependencies": {
    "http-proxy-middleware": "^3.0.5",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.3",
    "serve": "^14.2.1"
  }
}
